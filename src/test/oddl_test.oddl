CREATE CLASS stu_ (
	name CHAR(10),
	gender CHAR(1),
	age INT,
	stuid LONG,
	isgraduate BOOLEAN,
	testfloat FLOAT
);

CREATE SELECTDEPUTY graduates
SELECT name AS gname, age AS gage 
FROM stu_
WHERE isgraduate = TRUE AND (A = FALSE OR B = 20);

CREATE CLASS droptest (
	test INT
);

DROP CLASS droptest;

